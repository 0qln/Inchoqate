<Page x:Class="Inchoqate.GUI.Main.Editor.EditorFlowChart"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Inchoqate.GUI.Main.Editor"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="EditorFlowChart"
      Background="{DynamicResource Background_1}">

    <Canvas>

        <local:FlowChartNode Width="244" 
                             Height="175"
                             Canvas.Top="50"
                             Canvas.Left="50"
                             Title="Abstract Node"/>

        <local:FCN_GrayScale Width="244" 
                             Height="175"
                             Canvas.Top="127"
                             Canvas.Left="400"/>

        <local:FlowChartNode x:Name="E_HeadNode"/>

    </Canvas>

</Page>
